<Window x:Class="WpfFarmsSupplier.SuppliersCRUDsingle"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfFarmsSupplier"
        mc:Ignorable="d"
        Title="SuppliersCRUDsingle" Height="447" Width="719" Loaded="Window_Loaded">
    <StackPanel x:Name="SupplierPanel">
        <TextBlock TextWrapping="Wrap" Text="supplierid"/>
        <TextBox Height="23" TextWrapping="Wrap" Text="{Binding SupplierID, Mode=OneWay}"/>
        <TextBox Height="23" TextWrapping="Wrap" Text="{Binding SupplierName, Mode=TwoWay}"/>


    </StackPanel>
</Window>
